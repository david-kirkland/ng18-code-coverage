{
  "name": "ng18-code-coverage",
  "version": "1.2.0",
  "engines": {
    "npm": "10.8.1",
    "node": "v22.4.1"
  },
  "scripts": {
    "build": "ng build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ng": "ng",
    "serve:ssr:ng18-code-coverage": "node dist/ng18-code-coverage/server/server.mjs",
    "start": "ng serve",
    "test:all": "npm run test:component && npm run test:e2e",
    "test:clean": "rm -r coverage/* && rm -r .nyc_output/*",
    "test:clean:start": "npm run test:clean && rm -r .nyc_output_data/*",
    "test:component:data": "nyc report --reporter=json --report-dir=./.nyc_output_data/component",
    "test:component": "tsc && nyc --silent cypress run --component && npm run test:component:data",
    "test:coverage": "npm run test:clean:start && npm run test:all && npm run test:clean && npm run test:data:merge && npm run test:report",
    "test:data:merge": "npm run test:data:move && nyc merge .nyc_output_data .nyc_output/out.json",
    "test:data:move": "npm run test:data:move:component && npm run test:data:move:e2e",
    "test:data:move:component": "mv .nyc_output_data/component/coverage-final.json .nyc_output_data/data.component.json",
    "test:data:move:e2e": "mv .nyc_output_data/e2e/coverage-final.json .nyc_output_data/data.e2e.json",
    "test:e2e:data": "nyc report --reporter=json --report-dir=./.nyc_output_data/e2e/",
    "test:e2e": "tsc && nyc --silent ng e2e --headless --watch=false && npm run test:e2e:data",
    "test:report": "nyc report",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.1.0",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "18.0.0",
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@cypress/code-coverage": "3.12.42",
    "@cypress/schematic": "2.5.2",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "cypress": "13.13.1",
    "istanbul-lib-coverage": "3.2.2",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nyc": "17.0.0",
    "path": "0.12.7",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "5.5.3"
  }
}
